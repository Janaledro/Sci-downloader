	<../config.mk
all:VQ:
	set -x
	bin/targets | xargs mk 


results/PUBMED_PDFS.tsv: data/
	mkdir -p `dirname $target`
	echo "PMID	TITLE	PDF" > $target.build
	while read pmid
	do	
		echo "Generando fila para $pmid"
		PMID="$pmid"
		TITLE="$(grep "<title>" $pmid | tr " " "_" | cut -d ">" -f2)"
		#AUTHOR=
		#YEAR=
		PDF="$(echo $BASE_URL)"
	echo "$PMID" >> $target.build
	done < $prereq \
	&& mv $target.build $target
